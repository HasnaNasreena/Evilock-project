
pragma solidity >=0.5.0 <0.9.0;

// Creating a Smart Contract
contract Structreq{

// Structure of employee
struct allocreq{
	// State variables
    uint bid;
    string cid;
	string eid;
	string  sid;
	string casename;
	string evd;
	string file;
	string details;
	string date;

}
allocreq []emps;
// Function to add
// addrecords details
function addreq(uint bid, string memory cid,string memory eid,string memory sid,string memory casename,string memory evd,string memory file,string memory details,string memory date) public{
	allocreq memory e =allocreq(bid,cid,eid,sid,casename,evd,file,details,date);
	emps.push(e);
}

}
